{
  "name": "expo-api-route",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "EXPO_ROUTER_APP_ROOT=src/app expo start",
    "android": "EXPO_ROUTER_APP_ROOT=src/app expo start --android",
    "ios": "EXPO_ROUTER_APP_ROOT=src/app expo start --ios",
    "web": "EXPO_ROUTER_APP_ROOT=src/app expo start --web",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "env:pull:dev": "eas env:pull --environment development --path .env",
    "env:pull:preview": "eas env:pull --environment preview --path .env",
    "env:pull:prod": "eas env:pull --environment production --path .env",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:web": "eas build --profile production --platform web",
    "build:preview:web": "eas build --profile preview --platform web",
    "build:dev:web": "eas build --profile development --platform web",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@neondatabase/serverless": "^1.0.2",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/react-query": "^5.90.12",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.45.1",
    "expo": "~54.0.30",
    "expo-constants": "~18.0.12",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-unistyles": "^3.0.20",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "drizzle-kit": "^0.31.8",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "react-test-renderer": "19.1.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
